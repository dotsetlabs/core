        "core",
        "shared"
    ],
    "author": "dotset labs <hello@dotsetlabs.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dotsetlabs/core.git"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "dependencies": {
        "yaml": "^2.3.4"
    },
    "devDependencies": {
        "@types/node": "^22.0.0",
        "typescript": "^5.6.0",
        "vitest": "^2.1.0"
    }
