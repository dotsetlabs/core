{
    "name": "@dotsetlabs/core",
    "version": "1.0.0",
    "description": "Shared core utilities for dotset labs CLIs",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        }
    },
    "files": [
